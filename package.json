{
  "name": "github-search",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "transform:global": "cd src/assets/design-system && token-transformer tokens/ds-exported-figma-tokens.json tokens/global.json global --resolveReferences=false --expandTypography=true",
    "transform:light": "cd src/assets/design-system && token-transformer tokens/ds-exported-figma-tokens.json tokens/light.json light --resolveReferences=false",
    "transform:dark": "cd src/assets/design-system && token-transformer tokens/ds-exported-figma-tokens.json tokens/dark.json dark --resolveReferences=false",
    "build:tokens": "cd src/assets/design-system && node build.js",
    "build:theme": "npm run transform:global && npm run transform:light && npm run transform:dark && npm run build:tokens"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/vue": "^8.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "style-dictionary": "^3.9.0",
    "tailwindcss": "^3.3.5",
    "token-transformer": "^0.0.33",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
